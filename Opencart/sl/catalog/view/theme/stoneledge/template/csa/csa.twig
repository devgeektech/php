{{ header }}
<div class="page-content-header news recipe">
    <div class="container">
        <div class="row intro-heading">
            <div class="col-lg-12">
                <p>welcome to</p>
                <h1 class="pageTitle">{{ heading_title }}</h1>
                <ul class="breadcrumb">
                    {% for breadcrumb in breadcrumbs %}
                        {% if breadcrumb.href %}
                            <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                        {% else %}
                            <li>{{breadcrumb.text}}</li>
                        {% endif %}    
                    {% endfor %}
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <div class="page-content-body">
            <div class="container page-content-wrapper">
                <div class="csa-body clearfix">
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="csa-feedboxLeft">

                                {#<img src="{{ csa_image }}" alt="{{ heading_title }}" width="350" border="0">#}
                                
                                <div class="heading"><span>Pickup Address</span></div>
                                    <p> {{ pickup_address }} </p>
                                    <hr>

                                    <div class="heading"><span>Operating Hours</span></div>
                                    <p>{{ operating_hours }}</p>
                                    <hr>
                                    
                        {% if csa_email %}
                                    <div class="heading">
                                        <span>Contact Email</span>
                                        <a href="mailto:{{ csa_email }}">Send Message</a>
                                    </div>
                        {% endif %}
                        {% if csa_phone %}
                                    <div class="heading">
                                        <span>Onsite Phone</span>
                                        {{ csa_phone }}
                                    </div>
                        {% endif %}
                        {% if website %}
                                    <div class="heading">
                                        <span>Website</span>
                                        <a href="{{ website }}" target="_blank">Visit</a>
                                    </div>
                        {% endif %}
                        {% if brochure_link %}
                                    <div class="heading">
                                        <span>Brochure</span>
                                        <a href="{{ brochure_link }}" target="_blank">Download</a>
                                    </div>
                        {% endif %}
                                    
                        {% if latitude and longitude %}            
                                    <div class="heading">
                                        <span>Location</span> <a href="http://maps.google.com/maps?q={{ latitude }},{{ longitude }}" target="_blank">Directions</a></div>                                    
                                </div>
                        {% endif %}
                                
                            </div>
                            <div class="col-lg-8">
                                {#<h2>About {{ heading_title }} </h2>#}
                                <div class="csa-feedboxRight"><p></p><p>{{ description }}</p><br/></div>
                                <h2>Membership Requirements</h2>
                                <div class="csa-feedboxRequirements">
                                    <p>{{ membership_requirements }}</p>
                                </div>
                                
                                {#<div class="csa-feedboxRequirements">
                                    <p>{{ checkout_volunteer_messages }}</p></div>#}
                                
                                <div class="csa-feedboxButton">
                                    <a class="greenborder-cta" href="{{ register_link }}">New Member Waiting List â€º</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


            <div class="widget-panel">
                <div class="container-fluid" style="padding:0px;">
				{{ content_bottom }}
                    
                        <div class="col-md-4 block">
                            <h2>Instagram</h2> <a href="https://www.instagram.com/stoneledge.farm/" class="greenborder-cta ctaHeading" target="_blank">See All &rsaquo;</a>
                            <div class="clear"></div>
                            <p>Don't forget to follow us on Instagram to catch a glimpse at life on the farm!</p>
                            <div class="block-item instagram">

                                <div class="postbox">

                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 block">
                                <div class="block-item logos">
                                    <div class="owl-carousel owl-theme" id="logoslider">

                                        <div class="item"><img src="image/catalog/Pure-Catskills-923335510271017.png" /></div>

                                        <div class="item"><img src="image/catalog/Parnter-Charter-60221911271017.png" /></div>

                                        <div class="item"><img src="image/catalog/image-2361.png" /></div>

                                        <div class="item"><img src="image/catalog/USDA-Organic-906541011271017.png" /></div>

                                        <div class="item"><img src="image/catalog/Pride-of-New-York-591351111271017.png" /></div>

                                        <div class="item"><img src="image/catalog/image-8842.png" /></div>


                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="widget-panel-wrapper"></div>
                    <div class="farm-sketch"></div>
                </div>
<script>
    $(document).ready(function() {
		$('.parallax-window').parallax({imageSrc: '{{ store_url }}image/catalog/seedling-2021195556295.jpg'});
    });
</script>
{{ footer }}