{{ header }}
<div class="page-content-header">
		<div class="intro-heading">
			<h1 class="pageTitle">Farm Recipe</h1>
			<p>Check back regularly to find out the goings on at our certified organic farm!</p>
			<p>Select a category:</p>
			
			<form class="plain-select" id="" action="" method="post">
				<select id="goTo" name="goTo">
					<option value="#">All Recipe</option>
				</select>
			</form>
			
			
		</div>
</div>
<div class="page-content-body">
			<div class="container page-content-wrapper">
				<div class="row">
					<div class="col-lg-12">
<ul class="row recipe-grid clearfix">
		{% for recipe in all_Recipe %}	
		<li class="col-sm-3">
			<a href="{{ recipe.view }}" title=" {{ recipe.title }}">
				<img src="{{ recipe.image }}" alt="{{ recipe.title }}">
				<div class="info">
					<div class="name">{{ recipe.title }}</div>
					<div class="category">
						<ul>
							
						</ul>
					</div>
				</div>
			</a>
		</li>


				
						{% endfor %}	
							</div>
					</div>
					</div>
					</div>
					</div>

					
     <div class="widget-panel">
	<div class="container-fluid" style="padding:0px;">
	{{ content_bottom }}		

				

      

    <div class="col-md-4 block">
      <h2>Instagram</h2> <a href="https://www.instagram.com/stoneledge.farm/" class="greenborder-cta ctaHeading" target="_blank">See All &rsaquo;</a>
					<div class="clear"></div>
					<p>Don't forget to follow us on Instagram to catch a glimpse at life on the farm!</p>
					<div class="block-item instagram">
						
						<div class="postbox">

						</div>
					</div>
  </div>

      </div>

	   <div class="container">
        <div class="row">
            <div class="col-lg-12 block">
                <div class="block-item logos">
                    <div class="owl-carousel owl-theme" id="logoslider">
                        
                        <div class="item"><img src="image/catalog/Pure-Catskills-923335510271017.png" /></div>
                        
                        <div class="item"><img src="image/catalog/Parnter-Charter-60221911271017.png" /></div>
                        
                        <div class="item"><img src="image/catalog/image-2361.png" /></div>
                        
                        <div class="item"><img src="image/catalog/USDA-Organic-906541011271017.png" /></div>
                        
                        <div class="item"><img src="image/catalog/Pride-of-New-York-591351111271017.png" /></div>
                        
                        <div class="item"><img src="image/catalog/image-8842.png" /></div>
                        
                        
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
 
      <div class="widget-panel-wrapper"></div>
      <div class="farm-sketch"></div>
    </div>

<div id="information-information" class="container">
  <div class="row">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9' %}
    {% else %}
    {% set class = 'col-sm-12' %}
    {% endif %}
    <div class="{{ class }}">{{ content_top }}
	  {{ content_bottom }}</div>
    {{ column_right }}</div>
</div>
{{ footer }}
